{
  "name": "cmder-here",
  "displayName": "cmder-here",
  "description": "Add cmder here in ctx menu.",
  "version": "0.0.1",
  "publisher": "zyq",
  "engines": {
    "vscode": "^1.33.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:cmder-here.cmder here"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "cmder-here configuration",
      "properties": {
        "cmder-here.cmderLocation": {
          "type": "string",
          "default": "C://cmder/Cmder.exe",
          "description": "et to the full path of your Cmder.exe"
        }
      }
    },
    "commands": [
      {
        "command": "cmder-here.cmder here",
        "title": "cmder here"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "cmder-here.cmder here",
          "when": "explorerResourceIsFolder",
          "group": "cmder here@1"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.3.1",
    "vscode": "^1.1.28",
    "tslint": "^5.12.1",
    "@types/node": "^10.12.21",
    "@types/mocha": "^2.2.42"
  },
  "repository": "https://github.com/ZyqGitHub1/cmder-here"
}
